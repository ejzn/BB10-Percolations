<?xml version="1.0" encoding="utf-8"?>
<qnx>
  <id>HackerNewsBB10</id>
  <name>Hacker News BB10</name>
  <versionNumber>0.0.1</versionNumber>
  <author>Erik Johnson</author>
  <asset entry="true" type="qnx/elf">wwe</asset>
  <asset>options</asset>
  <asset>wwe</asset>
  <asset>chrome/index.html</asset>
  <initialWindow>
    <systemChrome>none</systemChrome>
    <transparent>true</transparent>
  </initialWindow>
  <env value="12" var="WEBKIT_NUMBER_OF_BACKINGSTORE_TILES"/>
  <permission system="true">run_native</permission>
  <description>This app is a pure JS bound to native application in the webview. The goal is to show the performance of the WebView on BB10</description>
  <action>read_device_identifying_information</action>
  <action>access_internet</action>
</qnx>
